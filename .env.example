# Resend API Key - Get yours at https://resend.com/api-keys
RESEND_API_KEY=

# Your business email where contact form messages will be sent
BUSINESS_EMAIL=

# Sentry Configuration - Get these from your Sentry project settings
# Used for source map uploads during the build process
SENTRY_ORG=
SENTRY_PROJECT=

# The DSN tells the Sentry SDK where to send events.
# This is a public key, so it's safe to expose it in the browser.
NEXT_PUBLIC_SENTRY_DSN=

# Redis connection URL
REDIS_URL=
